<div>
 <%= link_to 'Create advertisement', advertisements_path,
  class: 'btn btn-primary btn-lg active' %>
</div>
<br>
<div class="row-fluid">
    <% @advertisements.each do |a| %>
        <fieldset>
          <div class="span4 well">
           <div class= "field">
             <strong><%= a.title %></strong>
           </div>
           <br>
           <div class="field">
            <%= a.body %>
           </div>
          </div>
        </fieldset>
    <% end %>
</div>
